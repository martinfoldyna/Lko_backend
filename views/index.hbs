    <main>
        <section id="subjects">
            <h3 class="text-uppercase">Multimédia</h3>
            <div style="max-width: 95%; display: block; margin: 0 auto;">
                <div class="row">
                    <div class="uk-child-width-1-2@s uk-child-width-1-3@m uk-grid-row-large" uk-grid >
                        {{# each videos}}
                            {{>url-card input=this}}

                        {{/each}}

                        {{# each images}}
                            {{>image input=this}}

                        {{/each}}


                    </div>
                </div>
                <div class="">
                    <a href="/images" class="uk-button uk-button-primary rounded-corners link-button">Zobrazit více</a>
                </div>
            </div>

            <hr class="uk-divider-icon">

            <h3 class="text-uppercase">Webové stránky</h3>
            <div class="row" style="max-width: 95%; display: block; margin: 0 auto;">
                <div class="uk-child-width-1-2@s uk-child-width-1-3@m" uk-grid >
                    {{# each websites}}

                        {{>url-card input=this}}


                    {{/each}}
                </div>

                    <div class="">
                        <a href="/websites" class="uk-button uk-button-primary rounded-corners link-button">Zobrazit více</a>
                    </div>

            </div>

            <hr class="uk-divider-icon">
            <h3 class="text-uppercase">Technické výkresy</h3>
            <div class="row" style="max-width: 95%; display: block; margin: 0 auto;">
                <div class="uk-child-width-1-2@s uk-child-width-1-3@m" uk-grid >
                    {{# key_value drawings}}
                        <div>
                            <div class="uk-card uk-card-default uk-card-hover rounded-corners">
                                <div class="uk-card-media-top">
                                    <div class="uk-overflow-hidden">
                                        <div class="uk-position-relative uk-visible-toggle uk-light" tabindex="-1" uk-slideshow="autoplay: true">

                                            <ul class="uk-slideshow-items" style="min-height: 100%!important;width:100%;">
                                                {{#each value}}
                                                    <li>
                                                        <a data-fancybox data-caption="{{caption}}" href="{{this}}">
                                                            <img data-src="{{this}}" alt="" style="width: 100%; object-fit: contain" class="thumbnail-image" uk-cover uk-img="target: !.uk-slideshow-items">
                                                        </a>
                                                    </li>
                                                {{/each}}
                                            </ul>

                                            <a class="uk-position-center-left uk-position-small gallery-arrows" href="#" uk-slidenav-previous uk-slideshow-item="previous"></a>
                                            <a class="uk-position-center-right uk-position-small gallery-arrows" href="#" uk-slidenav-next uk-slideshow-item="next"></a>

                                        </div>
                                    </div>
                                </div>
                                <div class="uk-card-body">
                                    <h3>{{key}}</h3>
                                </div>
                            </div>
                        </div>
                    {{/ key_value}}


                </div>
                <div>
                    <a href="/drawings" class="uk-button uk-button-primary rounded-corners link-button">Zobrazit více</a>
                </div>
            </div>

        </section>
    </main>

