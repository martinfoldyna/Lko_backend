
<div class="card-wrapper">
    <a href="{{url}}" style="color: inherit; text-decoration: none;" target="_blank" >
        <div class="uk-card uk-card-default uk-card-hover rounded-corners">
            <div class="uk-card-media-top">

                <div class="uk-overflow-hidden">
                    <img src="data:image/jpg;base64,{{thumbnail}}" alt="" class="thumbnail-image">
                </div>

            </div>
            <div class="uk-card-body">
                <h2 class="uk-card-title">{{title}}</h2>
                {{# if (eq subject "MME")}}
                    <code class="uk-text-primary">#video</code>
                    {{else}}
                    <code class="uk-text-primary">#webová stránka</code>
                {{/if}}
                <code class="uk-text-primary">#{{classYear}}. ročník</code>
            </div>
        </div>
    </a>
</div>
