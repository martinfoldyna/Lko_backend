<p class="small-site-nav"><a href="/">Nástěnka</a> / Webové stránky</p>
{{!-- <h2>Webové stránky</h2> --}}
<div class="row">
    <div uk-filter="target: .js-filter">
        <ul class="uk-subnav uk-subnav-pill">
            <li class="uk-active" uk-filter-control><a href="#">Všechny</a></li>
            {{#each classYears}}
                <li uk-filter-control=".tag-{{this}}year"><a href="#">{{this}}. ročník</a></li>
            {{/each}}
        </ul>
        <div class="uk-child-width-1-2@s uk-child-width-1-3@m uk-child-width-1-4@l js-filter" uk-grid >
            {{#each websites}}
                <div class="tag-{{classYear}}year">
                    {{>url-card input=this}}
                </div>

            {{/each}}
        </div>
    </div>
</div>
